extends layout.pug

block content
  main.content
    .wrapper
      section.section
        .inner-block
          .inner-block__left
            .inner-header
              h1.inner-header__title Отзывы
            
              .reviews-websites
                .review-website
                  .review-website__name ProDoctorov.ru
                  .review__stars
                    img(src="./img/star.svg", alt="")
                    img(src="./img/star.svg", alt="")
                    img(src="./img/star.svg", alt="")
                    img(src="./img/star.svg", alt="")
